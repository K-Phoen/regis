{
    "name": "kphoen/regis",
    "license": "AGPL-3.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Regis\\": "src/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Functional\\": "tests/functional",
            "Tests\\Regis\\": "tests/unit"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.2.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",

        "robmorgan/phinx": "^0.9",

        "league/tactician-bundle": "~1.0",
        "league/tactician-logger": "^0.10",

        "ramsey/uuid": "^3.4",

        "squizlabs/php_codesniffer": "^2.6",
        "phpmd/phpmd": "^2.4",
        "phpstan/phpstan": "^0.8",

        "php-http/guzzle6-adapter": "^1.1",
        "knplabs/github-api": "^2.6",
        "gentle/bitbucket-api": "^1.1",
        "gitonomy/gitlib": "^1.0",

        "php-amqplib/php-amqplib": "^2.7",
        "swarrot/swarrot-bundle": "^1.5",

        "snc/redis-bundle": "2.x-dev",
        "predis/predis": "^1.1",

        "knpuniversity/oauth2-client-bundle": "^1.12",
        "league/oauth2-github": "^2.0",
        "stevenmaguire/oauth2-bitbucket": "^2.0",

        "friendsofsymfony/jsrouting-bundle": "^1.6",

        "kphoen/rulerz": "^0.20",
        "kphoen/rulerz-bundle": "^0.14",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "^6.4",
        "m6web/redis-mock": "^2.0",
        "phpstan/phpstan-doctrine": "^0.8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
